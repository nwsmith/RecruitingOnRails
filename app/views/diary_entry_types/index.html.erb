<p id="notice"><%= notice %></p>

<h1>Diary Entry Types</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Description</th>
      <th>Positive</th>
      <th>Negative</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diary_entry_types.each do |diary_entry_type| %>
      <tr>
        <td><%= diary_entry_type.code %></td>
        <td><%= diary_entry_type.name %></td>
        <td><%= diary_entry_type.description %></td>
        <td><%= diary_entry_type.positive %></td>
        <td><%= diary_entry_type.negative %></td>
        <td><%= link_to 'Show', diary_entry_type %></td>
        <td><%= link_to 'Edit', edit_diary_entry_type_path(diary_entry_type) %></td>
        <td><%= link_to 'Destroy', diary_entry_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

