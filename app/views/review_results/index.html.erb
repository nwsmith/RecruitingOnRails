<h1>Listing review_results</h1>

<table>
  <tr>
    <th>Code</th>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @review_results.each do |review_result| %>
  <tr>
    <td><%= review_result.code %></td>
    <td><%= review_result.name %></td>
    <td><%= review_result.description %></td>
    <td><%= link_to 'Show', review_result %></td>
    <td><%= link_to 'Edit', edit_review_result_path(review_result) %></td>
    <td><%= link_to 'Destroy', review_result, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Review result', new_review_result_path %>
