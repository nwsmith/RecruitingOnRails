<h1>Listing interviews</h1>

<table>
  <tr>
    <th>Meeting time</th>
    <th>Notes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @interviews.each do |interview| %>
  <tr>
    <td><%= interview.meeting_time %></td>
    <td><%= interview.notes %></td>
    <td><%= link_to 'Show', interview %></td>
    <td><%= link_to 'Edit', edit_interview_path(interview) %></td>
    <td><%= link_to 'Destroy', interview, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

