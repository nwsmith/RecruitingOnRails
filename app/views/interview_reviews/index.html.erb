<h1>Listing interview_reviews</h1>

<table>
  <tr>
    <th>Interview</th>
    <th>Reviewer</th>
    <th>Approved</th>
    <th>Unapproved</th>
    <th>Notes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @interview_reviews.each do |interview_review| %>
  <tr>
    <td><%= interview_review.interview.name %></td>
    <td><%= interview_review.user.name %></td>
    <td><%= interview_review.approved? %></td>
    <td><%= interview_review.unapproved? %></td>
    <td><%= interview_review.notes %></td>
    <td><%= link_to 'Show', interview_review %></td>
    <td><%= link_to 'Edit', edit_interview_review_path(interview_review) %></td>
    <td><%= link_to 'Destroy', interview_review, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Interview review', new_interview_review_path %>
